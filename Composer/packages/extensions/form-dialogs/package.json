        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "react-dev-utils": "7.0.3",
        "rimraf": "^2.6.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.0.1",
        "typescript": "^3.5.3",
        "webpack": "^4.32.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.2",
