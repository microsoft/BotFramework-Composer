    "author": "Microsoft",
    "description": "Botframework Composer dialog form components.",
    "main": "./lib/index.js",
    "typings": "./lib/VisualSchemaEditor.d.ts",
    "files": [
        "lib/*index*",
        "lib/VisualSchemaEditor.d.ts",
        "LICENSE"
    ],
    "scripts": {
        "clean": "rimraf lib dist",
        "start": "node tools/devServer.js",
        "build:app": "rimraf dist && cross-env NODE_ENV=production BUILD_DEV=false webpack -p",
        "build": "rimraf lib && webpack --config webpack.lib.config.js --mode production",
        "lint": "eslint --quiet ./src",
        "lint:fix": "yarn lint --fix",
        "extract": "cross-env NODE_ENV=production format-message extract -g underscored_crc32 -o locales/en-US.json src/**/*.ts src/**/*.tsx"
    },
    "dependencies": {
        "@fluentui/react": "^7.121.11",
