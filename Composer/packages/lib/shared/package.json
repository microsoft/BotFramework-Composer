{
  "name": "@bfc/shared",
  "version": "0.0.0",
  "description": "<description>",
  "main": "lib/index.js",
  "repository": "https://github.com/microsoft/BotFramework-Composer",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "yarn clean && yarn build:css && yarn build:ts",
    "build:ts": "tsc --build tsconfig.build.json",
    "build:css": "copyfiles --up 1 \"src/**/*.css\" \"src/**/*.scss\" lib",
    "clean": "rimraf lib demo/dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint --quiet --ext .js,.jsx,.ts,.tsx ./src ./__tests__",
    "lint:fix": "yarn lint --fix",
    "lint:typecheck": "tsc --noEmit",
    "watch": "yarn build:ts --watch"
  },
  "peerDependencies": {
    "format-message": "^6.2.3",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "@bfc/test-utils": "*",
    "@types/nanoid": "^2.1.0",
    "@types/react-dom": "16.9.5",
    "@types/react": "16.9.23",
    "autoprefixer": "^9.5.1",
    "copyfiles": "^2.1.0",
    "office-ui-fabric-react": "^7.88.1",
    "react-dom": "16.13.0",
    "react": "16.13.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "json-schema": "^0.2.5",
    "nanoid": "^3.1.3",
    "nanoid-dictionary": "^3.0.0"
  }
}
