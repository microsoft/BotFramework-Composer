    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/nanoid": "^2.1.0",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.5",
    "autoprefixer": "^9.5.1",
    "copyfiles": "^2.1.0",
    "jest": "24.0.0",
    "jest-dom": "^3.1.3",
    "office-ui-fabric-react": "^7.88.1",
