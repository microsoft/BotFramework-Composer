{
  "name": "composer-api-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "yarn build && node --inspect=9229 build/server.js",
    "start:watch": "nodemon",
    "test": "yarn typecheck && jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --quiet --ext .js,.jsx,.ts,.tsx ./src ./__tests__",
    "lint:fix": "yarn lint --fix"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "*.js",
      "*.json",
      ".git",
      ".vscode",
      "__tests__",
      "node_modules",
      "build"
    ],
    "exec": "yarn start",
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/preset-env": "7.3.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.2",
    "@types/express": "^4.16.1",
    "@types/globby": "^9.1.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.123",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.6",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-jest": "24.0.0",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "jest": "24.0.0",
    "nodemon": "^1.18.11",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "azure-storage": "^2.10.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "globby": "^9.1.0",
    "http-errors": "^1.7.2",
    "immer": "^2.1.4",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "morgan": "^1.9.1",
    "botbuilder-lg": "https://botbuilder.myget.org/F/botbuilder-declarative/npm/botbuilder-lg/-/4.5.1.tgz"
  }
}
