{
  "name": "@bfc/server",
  "license": "MIT",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/public/static/js/*.js'",
    "build": "tsc -p tsconfig.build.json && node scripts/build.js",
    "clean": "rimraf build",
    "prep:dist:electron": "yarn build --outDir ../electron-server/dist && node scripts/copyAssets.js",
    "start": "node build/init.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --noImplicitAny --watch",
    "lint": "eslint --quiet ./src",
    "lint:fix": "yarn lint --fix"
  },
  "author": "",
  "nodemonConfig": {
    "exec": "cross-env TS_NODE_FILES=true node --inspect=9228 -r ts-node/register src/init.ts",
    "watch": [
      "src",
      "../extension/lib"
    ],
    "ext": "ts",
    "delay": 2
  },
  "devDependencies": {
    "@botframework-composer/test-utils": "*",
    "@types/archiver": "^3.1.0",
    "@types/body-parser": "^1.17.0",
