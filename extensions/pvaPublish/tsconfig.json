{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types/",
    ],
    "types": ["jest"]
  },
  "include": [
    "src/**/*",
    "testing"
  ],
  "exclude": [
    "node_modules",
  ]
}
