{
  "name": "azure-publish-new",
  "version": "1.0.0",
  "description": "Publish bot to an Azure Web App",
  "main": "dist/extension.js",
  "license": "MIT",
  "scripts": {
    "build": "node ../scripts/build.js",
    "watch": "yarn build --watch"
  },
