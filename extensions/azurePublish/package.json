    "@types/request": "2.48.8",
    "@types/request-promise": "4.1.48",
    "html-webpack-plugin": "5.5.0",
    "ts-loader": "9.3.1",
    "typescript": "^3.9.6",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
