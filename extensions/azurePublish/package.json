{
  "name": "azurePublish",
  "version": "1.0.0",
  "description": "Publish bot to an Azure Web App (Preview)",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build:appinsight": "cd ../lib/arm-appinsights && yarn && yarn build",
    "build:ui": "webpack --config webpack.config.js",
    "build:node": "tsc --project tsconfig.node.json",
    "build": "yarn build:node && yarn build:ui"
  },
  "extendsComposer": true,
  "composer": {
    "bundles": [
      {
        "id": "publish",
        "path": "lib/publish.js"
      }
    ],
    "contributes": {
      "views": {
        "publish": [{
          "bundleId": "publish"
        }]
      }
    }
  },
  "dependencies": {
    "@azure/arm-appinsights": "../lib/arm-appinsights",
    "@azure/arm-appservice": "^6.0.0",
    "@azure/arm-appservice-profile-2019-03-01-hybrid": "1.0.0",
    "@azure/arm-botservice": "^2.0.0",
    "@azure/arm-cognitiveservices": "^5.2.0",
    "@azure/arm-cosmosdb": "^9.0.0",
    "@azure/arm-deploymentmanager": "3.0.0",
    "@azure/arm-resources": "2.1.0",
    "@azure/arm-search": "^1.3.0",
    "@azure/arm-storage": "^15.0.0",
    "@azure/arm-subscriptions": "2.0.0",
    "@azure/cognitiveservices-luis-authoring": "4.0.0-preview.1",
    "@azure/cosmos": "^3.7.4",
    "@azure/graph": "5.0.1",
    "@azure/ms-rest-browserauth": "0.1.4",
    "@azure/ms-rest-js": "^2.0.7",
    "@azure/ms-rest-nodeauth": "3.0.3",
    "@bfc/code-editor": "../../Composer/packages/lib/code-editor",
    "@bfc/extension": "../../Composer/packages/extension",
    "@bfc/extension-client": "../../Composer/packages/extension-client",
    "@bfc/indexers": "../../Composer/packages/lib/indexers",
    "@bfc/shared": "../../Composer/packages/lib/shared",
    "@bfc/types": "../../Composer/packages/types",
    "@microsoft/bf-lu": "^4.11.0-dev.20201005.7e5e1b8",
    "adal-node": "0.2.1",
    "archiver": "^5.0.2",
    "axios": "^0.19.2",
    "fs-extra": "8.1.0",
    "lodash": "^4.17.20",
    "md5": "2.2.1",
    "minimist": "1.2.5",
    "office-ui-fabric-react": "^7.145.0",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "url-loader": "^4.1.1",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@types/archiver": "3.1.0",
    "@types/fs-extra": "8.1.0",
    "@types/request": "2.48.4",
    "@types/request-promise": "4.1.45",
    "@types/node": "^14.11.2",
    "html-webpack-plugin": "^4.3.0",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "resolutions": {
    "bl": "^4.0.3"
  }
}
