{
  "$type": "Microsoft.AdaptiveDialog",
  "$designer": {
    "createdAt": "2019-07-03T06:49:41.680Z",
    "updatedAt": "2019-07-16T18:48:54.243Z",
    "id": "607738",
    "name": "RepeatDialog"
  },
  "autoEndDialog": true,
  "$schema": "../../app.schema",
  "rules": [
    {
      "$type": "Microsoft.EventRule",
      "$designer": {
        "name": "Handle an Event",
        "id": "695225",
        "updatedAt": "2019-07-22T07:45:52.118Z"
      },
      "events": [
        "beginDialog"
      ],
      "steps": [
        {
          "$type": "Microsoft.ConfirmInput",
          "$designer": {
            "createdAt": "2019-07-16T18:52:10.284Z",
            "updatedAt": "2019-07-16T18:52:14.640Z",
            "id": "841600",
            "name": "Prompt for confirmation"
          },
          "property": "user.confirmed",
          "prompt": "Do you want to repeat this dialog, yes to repeat, no to end this dialog",
          "unrecognizedPrompt": "I need a yes or no.",
          "maxTurnCount": 0,
          "alwaysPrompt": true,
          "allowInterruptions": false,
          "defaultLocale": "en-us",
          "style": "Auto",
          "choiceOptions": {
            "inlineSeparator": ", ",
            "inlineOr": " or ",
            "inlineOrMore": ", or ",
            "includeNumbers": true
          }
        },
        {
          "$type": "Microsoft.IfCondition",
          "$designer": {
            "createdAt": "2019-07-16T18:52:16.346Z",
            "updatedAt": "2019-07-16T18:52:18.539Z",
            "id": "365292",
            "name": "Branch: If/Else"
          },
          "condition": "user.confirmed",
          "steps": [
            {
              "$type": "Microsoft.RepeatDialog",
              "$designer": {
                "createdAt": "2019-07-16T18:52:20.435Z",
                "updatedAt": "2019-07-16T18:52:23.947Z",
                "id": "221664",
                "name": "Repeat this Dialog"
              }
            }
          ],
          "elseSteps": [
            {
              "$type": "Microsoft.EndDialog",
              "$designer": {
                "createdAt": "2019-07-16T18:52:25.645Z",
                "updatedAt": "2019-07-16T18:52:28.521Z",
                "id": "573415",
                "name": "End Dialog"
              }
            }
          ]
        }
      ]
    }
  ]
}