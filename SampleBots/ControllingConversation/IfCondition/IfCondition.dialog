{
  "$type": "Microsoft.AdaptiveDialog",
  "autoEndDialog": true,
  "steps": [
    {
      "$type": "Microsoft.NumberInput",
      "prompt": "Hello, What's your age?",
      "property": "user.age"
    },
    {
      "$type": "Microsoft.IfCondition",
      "condition": "user.age >= 18",
      "steps": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "You are an adult."
        }
      ],
      "elseSteps": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "You are under 18."
        }
      ]
    }
  ],
  "$schema": "../../app.schema",
  "$designer": {
    "createdAt": "2019-07-03T06:49:39.154Z",
