{
  "$type": "Microsoft.AdaptiveDialog",
  "autoEndDialog": true,
  "steps": [
    {
      "$type": "Microsoft.ChoiceInput",
      "property": "user.name",
      "choices": [
        {
          "value": "Susan"
        },
        {
          "value": "Nick"
        },
        {
          "value": "Tom"
        }
      ],
      "prompt": "Please select a value from below:",
      "style": "List",
      "alwaysPrompt": true
    },
    {
      "$type": "Microsoft.SendActivity",
      "activity": "Hello {user.name}."
    },
    {
      "$type": "Microsoft.SwitchCondition",
      "condition": "user.name",
      "cases": [
        {
          "value": "Susan",
          "steps": [
            {
              "$type": "Microsoft.SendActivity",
              "activity": "You are the CEO."
            }
          ]
        },
        {
          "value": "Nick",
          "steps": [
            {
              "$type": "Microsoft.SendActivity",
              "activity": "You are the CFO."
            }
          ]
        },
        {
          "value": "Tom",
          "steps": [
            {
              "$type": "Microsoft.SendActivity",
              "activity": "You are the CTO."
            }
          ]
        }
      ]
    }
  ],
  "$schema": "../../app.schema",
  "$designer": {
    "createdAt": "2019-07-03T06:49:45.210Z",
    "updatedAt": "2019-07-03T06:49:45.249Z",
    "id": "122121"
  }
}
